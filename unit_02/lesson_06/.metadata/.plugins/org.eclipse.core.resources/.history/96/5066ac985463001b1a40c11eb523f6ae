#include<stdio.h>
#include<stdlib.h>
#include<string.h>
#include<conio.h>



#define EMPLOYEE_NUMBER 2




struct SEmployee
{
	char name[100];
	unsigned int salary;
	unsigned int day;
	unsigned int month;
	unsigned int year;
}emp[EMPLOYEE_NUMBER];



void main() //main function
{

	int i = 0;


	do
	{
		printf("Enter Employee name: ");
		gets(emp->name[i]);






		i++;
	}while(getchar() == 'y');


	printf("%s%50s%50s", "Emplyee name", "Employee Birthday", "Employee Salary");
}
