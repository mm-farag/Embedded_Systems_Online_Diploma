#include <stdio.h>
#include <stdlib.h>

enum NUMGender
{
	male,
	female,
};

struct Sperson
{
	char name[50];
	enum NUMGender m_Gender;
};



int main(void)
{
	int i;
	struct Sperson person[] =
	{
			{"Mohamed", male}, {"Linda", female}, {"Nader", male}

	};

	printf("%d", sizeof(struct Sperson));

//	for(i= 0; i<sizeof(person)/sizeof(struct Sperson); i++)
//	{
//
//	}

	return EXIT_SUCCESS;
}
